{
  "id": "process-monitor",
  "version": "1.0.0",
  "name": "Process Monitor",
  "description": "Monitor WebDesk WM processes, events and system resources in real-time",
  "author": "marsuch",

  "entryPoint": "index.js",

  "ui": {
    "displayName": "Process Monitor",
    "icon": "systemMonitor",
    "category": "system",
